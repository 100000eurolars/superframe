{
  "name": "aframe-state-component",
  "version": "1.0.0",
  "description": "State management using declarative binding, pure function state transitions, and event-based action dispatchers, with Redux.",
  "main": "index.js",
  "unpkg": "dist/aframe-state-component.min.js",
  "scripts": {
    "build": "browserify examples/main.js -o examples/build.js",
    "dev": "budo examples/main.js:examples/build.js --port 8000 --live --open",
    "dist": "webpack index.js dist/aframe-state-component.js && webpack -p index.js dist/aframe-state-component.min.js",
    "postpublish": "npm run dist",
    "test": "karma start ./tests/karma.conf.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ngokevin/kframe.git"
  },
  "keywords": [
    "aframe",
    "aframe-component",
    "aframe-vr",
    "vr",
    "mozvr",
    "webvr"
  ],
  "author": "Kevin Ngo <me@ngokevin.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ngokevin/kframe/issues"
  },
  "homepage": "https://github.com/ngokevin/kframe/tree/components/redux#readme",
  "devDependencies": {
    "aframe-bmfont-text-component": "^0.1.4",
    "semistandard": "^8.0.0",
    "shelljs": "^0.7.0",
    "shx": "^0.1.1",
    "snazzy": "^4.0.0",
    "webpack": "^1.13.2"
  },
  "semistandard": {
    "ignore": [
      "examples/build.js",
      "dist/**"
    ]
  },
  "dependencies": {
    "redux": "^3.5.2"
  }
}
